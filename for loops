console.log("print all numbers btw -10 and 19");
for(i=-10;i<20;i++){
    console.log(i);
}
console.log("print all eben numbers btw 10 and 40");
for(i=10;i<41;i++){
    if(i%2===0){
        console.log(i);
    }
}
console.log("print all odd nuim btw 300 and 333");
for(i=300;i<334;i++){
    if(i%2!==0){
        console.log(i);
    }
}
console.log("print num div by 5 and 3 btw 5 and 50");
for(i=5;i<51;i++){
    if(i%5===0 && i%3===0){
        console.log(i);
    }
}
